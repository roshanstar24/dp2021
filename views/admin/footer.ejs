<script>
    $(document).ready(function () {
        var Toast = Swal.mixin({
            toast: true,
            position: 'top-end',
            showConfirmButton: false,
            timer: 5000
        });
        $('#profilelink').click(function (event) {
            aj = $(this)
            event.preventDefault();
            $.ajax({
                url: aj.attr('ahref'),
                method: 'post',
                data: "id=" + aj.attr('sendid'),
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $('#contentholder').html(data)
                }
            })
        })
        $('.editprod').click(function (event) {
            aj = $(this)
            console.log(aj.attr('ahref'))
            event.preventDefault();
            $.ajax({
                url: aj.attr('ahref'),
                method: 'post',
                data : "id=09de3388-624e-4188-a2e7-0df7432354f4",
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $('#contentholder').html(data)
                }
            })
        })

        $('body').on('click','.sendajax',function (event) {
            aj = $(this)
            console.log(aj.attr('ahref'))
            event.preventDefault();
            $.ajax({
                url: aj.attr('ahref'),
                method: 'get',
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $('#contentholder').html(data)
                }
            })
        })
    })
</script>
